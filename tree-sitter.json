{
	"$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
	"grammars": [
		{
			"name": "flow",
			"camelcase": "Flow",
			"scope": "source.flow",
			"file-types": ["ft"],
			"injection-regex": "^flow$",
			"class-name": "TreeSitterFlow"
		}
	],
	"metadata": {
		"version": "0.1.0",
		"license": "MIT",
		"description": "Flow Templates Tree-Sitter parser",
		"authors": [
			{
				"name": "@flowtemplates"
			}
		],
		"links": {
			"repository": "https://github.com/flowtemplates/tree-sitter-flow"
		}
	},
	"bindings": {
		"c": true,
		"go": true,
		"node": true,
		"python": true,
		"rust": true,
		"swift": true,
		"zig": false
	}
}
